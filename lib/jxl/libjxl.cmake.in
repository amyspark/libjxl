@PACKAGE_INIT@

if(NOT TARGET JPEGXL::jxl)
    include("${CMAKE_CURRENT_LIST_DIR}/JPEGXLTargets.cmake")
    get_target_property(JPEGXL_LIBRARY JPEGXL::jxl INTERFACE_LIBRARY)
    get_target_property(JPEGXL_THREADS_LIBRARY JPEGXL::jxl_threads INTERFACE_LIBRARY)
    set(JPEGXL_LIBRARIES "${JPEGXL_LIBRARY};${JPEGXL_THREADS_LIBRARY}")
    get_target_property(JPEGXL_INCLUDE_DIRS JPEGXL::jxl INTERFACE_INCLUDE_DIRECTORIES)
endif()
